    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: HttpRequestGeneratorClient",
            "cwd": "${workspaceFolder}/HttpRequestGeneratorClient",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "python": "${workspaceFolder}/HttpRequestGeneratorClient/.venv/Scripts/python.exe"
        },
        {
            "name": "Python: FlaskAPI",
            "cwd": "${workspaceFolder}/FlaskAPI",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "main.py",
                "FLASK_ENV": "development"
            },
            "args": [
                "run",
                "--no-debugger"
            ],
            "jinja": true,
            "justMyCode": true,
            "python": "${workspaceFolder}/FlaskAPI/.venv/Scripts/python.exe"
        }
    ]
}